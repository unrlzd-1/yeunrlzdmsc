{
  "version": 2,
  "headers": [
    {
      "source": "/songs/.+\\.(mp3|wav|ogg)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/upload", "destination": "/upload.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}